{
  "name": "koconut",
  "version": "1.0.15",
  "description": "Promise-Friendly Kotlin style collection lib for nodejs.",
  "main": "dist/index.internal.js",
  "types": "dist/index.internal.d.ts",
  "homepage": "https://apexcaptain.github.io/Koconut/",
  "keywords": [
    "nodejs",
    "typescript-library",
    "typescript",
    "collection",
    "promise",
    "iteration-process",
    "async-await",
    "data-structures"
  ],
  "engines": {
    "node": ">= 7.0"
  },
  "scripts": {
    "// Project": "Project Commands",
    "pro": "yarn pro@update && yarn pro@build && yarn pro@topic && yarn doc@build &&  && yarn test && ts-node ./commands/Distributer.task.ts",
    "pro@update": "ncu -u && yarn && echo Update Completed",
    "pro@build": "ts-node ./commands/DirectoryCleaner.task.ts && babel ./src --out-dir dist --extensions .ts,.tsx && tsc && echo Build Completed",
    "pro@topic": "ts-node ./commands/TopicsFetcher.task.ts && echo Topics Fethcing Completed",
    "// Document": "Documentization Commands",
    "doc": "yarn doc@build && yarn doc@view",
    "doc@build": "typedoc --options ./typedoc.json ./src && ts-node ./commands/TypeDocAfterRenderer.task.ts && echo Documentization Completed",
    "doc@view": "ts-node ./commands/LocalDocumentViewer.task.ts",
    "// Test": "Test Commands",
    "test": "echo Preparing Unit Test... && mocha -r ts-node/register ./test/unitTest/*.spec.ts && echo Unit Test Completed",
    "test@validate": "yarn test && ts-node ./commands/DepreactedMethodValidator.task.ts -f && echo Validating deprecation method completed.",
    "test@match-mocha-version": "ts-node ./commands/MochaDependencyMacher.task.ts",
    "test@sequence": "echo Preparing Unit Test... && mocha -r ts-node/register ./test/unitTest/*Sequence.spec.ts && echo Unit Test Completed",
    "test@sample": "ts-node ./test/Sample.test.ts && echo Sample Test Completed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ApexCaptain/Koconut.git"
  },
  "author": {
    "name": "SangHun Lee",
    "email": "ayteneve93@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ApexCaptain/Koconut/issues",
    "email": "ayteneve93@gmail.com"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.8"
  },
  "config": {
    "docViewPort": "5000"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@octokit/rest": "^18.7.0",
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.1",
    "babel-preset-minify": "0.5.1",
    "chai": "^4.3.4",
    "express": "4.17.1",
    "mocha": "^9.0.2",
    "npm-check-updates": "^11.8.3",
    "open": "^8.2.1",
    "ts-node": "^10.1.0",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  }
}
