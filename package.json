{
  "name": "koconut",
  "version": "1.0.15",
  "description": "Promise-Friendly Kotlin style collection lib for nodejs.",
  "main": "dist/index.internal.js",
  "types": "dist/index.internal.d.ts",
  "keywords": [
    "nodejs",
    "typescript-library",
    "typescript",
    "collection",
    "promise",
    "iteration-process",
    "async-await",
    "data-structures"
  ],
  "engines": {
    "node": ">= 7.0"
  },
  "scripts": {
    "update": "ncu -u && npm install && echo Update Completed",
    "build": "node ./dev/ClearTask.js && babel ./lib --out-dir dist --extensions .ts,.tsx && tsc && echo Build Completed",
    "doc": "typedoc --options ./typedoc.json ./lib && node ./dev/TypeDocNoJekyllCreatingTask.js && echo Documentization Completed",
    "topic": "node ./dev/TopicsFetchingTask.js && echo Topics Fethcing Completed",
    "test": "echo Preparing Unit Test... && mocha -r ts-node/register ./test/unitTest/*.spec.ts && echo Unit Test Completed",
    "validate": "npm run test && node ./dev/DepreactedMethodValidatingTask.js -f && echo Validating deprecation method completed.",
    "distribute": "npm run update && npm run build && npm run doc && npm run topic && npm test && node ./dev/DistributingTask",
    "sample": "ts-node ./test/Sample.test.ts && echo Sample Test Completed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ApexCaptain/Koconut.git"
  },
  "author": {
    "name": "SangHun Lee",
    "email": "ayteneve93@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ApexCaptain/Koconut/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2"
  },
  "homepage": "https://apexcaptain.github.io/Koconut/",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@octokit/rest": "^18.0.6",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "npm-check-updates": "^9.0.3",
    "npx": "^10.2.2",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  }
}
