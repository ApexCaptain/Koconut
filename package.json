{
  "name": "koconut",
  "version": "1.0.8",
  "description": "Promise-Friendly Kotlin style collection lib for nodejs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "update": "ncu -u && npm install",
    "build": "node ./etc/ClearTask.js && tsc",
    "doc": "typedoc --options ./typedoc.json ./lib",
    "distribute": "npm run update && npm run build && npm run doc && node ./etc/DistributingTask",
    "test": "mocha -r ts-node/register ./lib/spec/*.spec.ts",
    "sample": "ts-node ./sampleTest.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ApexCaptain/Koconut.git"
  },
  "author": {
    "name": "SangHun Lee",
    "email": "ayteneve93@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ApexCaptain/Koconut/issues"
  },
  "homepage": "https://github.com/ApexCaptain/Koconut#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.4",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "npm-check-updates": "^8.1.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  }
}
