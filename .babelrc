{
    "presets": [
        "@babel/env",
        ["@babel/preset-typescript", {"allowNamespaces" : true}],
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "@babel/syntax-dynamic-import",
        "@babel/plugin-transform-runtime",
        "@babel/transform-async-to-generator"
    ]
}